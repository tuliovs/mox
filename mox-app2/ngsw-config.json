{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ],
      "urls": [
        {"url": "https://fonts.googleapis.com/css?family=Exo+2:400,600,700|Quantico:400,700"}
]
    }
  }],
  "dataGroups": [
    {
      "name": "scryfall-img-api",
      "urls": ["https://img.scryfall.com/cards/"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "200h",
        "timeout": "20s"
      }
    }
  ]
}
