{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "dist/mox-app2",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/foo",
        "destination": "/bar",
        "type": 301
      },
      {
        "source": "/firebase/*",
        "destination": "https://firebase.google.com",
        "type": 302
      }
    ],
    "rewrites": []
  },
  "storage": {
    "rules": "storage.rules"
  }
}
